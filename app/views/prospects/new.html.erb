<%= form_for @prospect do |f| %>
    email <%= f.text_field :email %><br/>
    firstname <%= f.text_field :firstname %><br/>
    lastname <%= f.text_field :lastname %><br/>
    locale_id <%= f.text_field :locale_id %><br/>
    civ <%= f.text_field :civ %><br/>
    birth_at <%= f.text_field :birth_at %><br/>
    zip <%= f.text_field :zip %><br/>
    <%= f.submit :name => nil %>
<% end %>