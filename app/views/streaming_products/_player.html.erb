<% if token %>
    <% token_code = token.token %>
<% else %>
    <% if Token.dvdpost_ip?(request.remote_ip) || (current_customer && current_customer.super_user?) || (/^192(.*)/.match(request.remote_ip)) %>
        <% token_code = "#{streaming.hd? ? '4' : '3'}/i/" + streaming.imdb_id.to_s %>
    <% end %>
<% end %>
<%= flowplayer(filename, source, streaming, token_code, browser, @code, season_id, episode_id) %>